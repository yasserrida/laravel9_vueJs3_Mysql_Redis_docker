    "filepond-plugin-image-resize": "^2.0.10",
    "filepond-plugin-image-transform": "^3.8.7",
    "imask": "^6.4.3",
    "laravel-echo": "^1.14.1",
    "mapbox-gl": "^2.10.0",
    "markdown-it-emoji": "^2.0.2",
    "notyf": "^3.10.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.23",
    "socket.io": "^2.5.0",
    "socket.io-client": "^2.5.0",
    "tippy.js": "^6.3.7",
    "tslib": "^2.4.1",
