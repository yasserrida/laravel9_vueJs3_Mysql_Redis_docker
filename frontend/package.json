    "@vueuse/head": "^0.7.13",
    "ag-grid-community": "^27.3.0",
    "ag-grid-vue3": "^27.3.0",
    "apexcharts": "^3.36.0",
    "axios": "^0.24.0",
    "billboard.js": "^3.6.3",
    "bulma": "^0.9.4",
    "bulma-css-vars": "^0.7.0",
    "dayjs": "^1.11.6",
